    "import re\n",
    "def name_of_email(addr):\n",
    "    a = re.compile(r'<?([\\w\\s]+)>?([\\w]+)*@[\\w]+\\.\\w+')\n",
    "    b = a.match(addr)\n",
    "    return b.group(1)\n",
    "print(name_of_email('<Tom Paris>tom@voyager.org'))\n",
    "print(name_of_email('tom@voyager.org'))"
   ]
  }
